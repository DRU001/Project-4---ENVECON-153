    "# Choose sex/age group:\n",
    "group = \"M 19-30\"\n",
    "\n",
    "# Define *minimums*\n",
    "bmin = pd.read_csv('./diet_minimums.csv').set_index('Nutrition')[group]\n",
    "\n",
    "# Define *maximums*\n",
    "bmax = pd.read_csv('./diet_maximums.csv').set_index('Nutrition')[group]"
